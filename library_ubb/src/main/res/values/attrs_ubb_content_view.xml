<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="UBBContentView" tools:context=".widget.UBBContentView">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="android:lineSpacingExtra" />
        <attr name="android:lineSpacingMultiplier" />
        <attr name="verticalEdgeSpacing" format="dimension" />
        <attr name="android:verticalSpacing" />
        <attr name="android:horizontalSpacing" />
        <attr name="headSpacing" format="dimension" />
        <attr name="footSpacing" format="dimension" />
        <attr name="imageWidth" format="dimension">
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
        <attr name="imageCorners" format="dimension" />
        <attr name="imageCornersLeft" format="dimension" />
        <attr name="imageCornersLeftTop" format="dimension" />
        <attr name="imageCornersLeftBottom" format="dimension" />
        <attr name="imageCornersTop" format="dimension" />
        <attr name="imageCornersRight" format="dimension" />
        <attr name="imageCornersRightTop" format="dimension" />
        <attr name="imageCornersRightBottom" format="dimension" />
        <attr name="imageCornersBottom" format="dimension" />
        <attr name="imageCornersEnableFlags">
            <flag name="any" value="0x10" />
            <flag name="first" value="0x20" />
            <flag name="last" value="0x02" />
            <flag name="edge" value="0x22" />
        </attr>
        <attr name="imagePlaceholder" format="reference" />
        <attr name="imagePlaceholderRatio" format="string" />
        <attr name="header" format="reference" />
        <attr name="footer" format="reference" />
    </declare-styleable>
</resources>